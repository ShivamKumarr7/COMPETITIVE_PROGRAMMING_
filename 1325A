#include<bits/stdc++.h>
using namespace std;
#define ll long long 
#define mp make_pair
#define pb push_back
#define mod 1000000007
#define pi 3.14159265
#define setbits(x) __builtin_popcount(x)
#define trailing_zeros(x) __builtin_ctz(x)
const int Max = 1e5 + 5;
const int Bmax = 1e7 + 5;
#define ps(x,y) fixed<<setprecision(y)<<x 
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin>>t;
    while(t--)
    {
        ll x;
        cin>>x;
        if(x&1)
        {
            // odd number
            cout<<(x-1)<<" "<<(1)<<"\n";
        }
        else 
        {
            // even number 
            cout<<(x/2)<<" "<<(x/2)<<"\n";
        }
    }
    return 0;
}
