#include<bits/stdc++.h>
using namespace std;
#define MOD 1000000007 
#define ll long long 
#define PI 3.14159265 
#define setbits(x)  __builtin_popcount(x)
#define zrobits(x)  __builtin_ctzll(x)
#define ps(x,y)  fixed<<setprecision(y)<<x 
#define pb push_back
#define mp make_pair
const int Max = 1e5 + 5;
const int Bmax = 1e7 + 5;
int main()
{
    ios_base::sync_with_stdio(false);
    cin.tie(NULL);
    int t;
    cin>>t;
    while(t--)
    {
        string s;
        cin>>s;
        int first = 0;
        int second = 0;
        for(int i=0;i<=2;i++)
        {
            first += s[i] - '0';
        }
        for(int i=3;i<=5;i++)
        {
            second += s[i] - '0';
        }
        if(first == second)
        {
            cout<<"YES\n";
        }
        else 
        {
            cout<<"NO\n";
        }
    }
    return 0;
}
